<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_claim_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
            <com.zhy.autolayout.AutoRelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@color/white"
               >
                <TextView
                    android:layout_centerInParent="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="信息确认"
                    android:textColor="@color/black"
                    android:textSize="16sp" />
            </com.zhy.autolayout.AutoRelativeLayout>
            <View
                android:layout_width="match_parent"
                android:background="@color/col_ECEEF0"
                android:layout_height="1dp"/>
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none">
                <com.zhy.autolayout.AutoLinearLayout
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">
                    <com.zhy.autolayout.AutoLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="100px"
                        android:gravity="center_vertical">
                        <TextView
                            style="@style/claimInfo_tv_styles"
                            android:text="推荐ID:"/>
                        <ImageView
                            android:layout_marginLeft="2dp"
                            android:layout_marginRight="2dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/redsure" />
                        <EditText
                            android:id="@+id/et_verifyInfo_recommendId"
                            style="@style/claimInfo_et_styles"
                            />
                    </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_verifyInfo_name"
                    style="@style/claimInfo_tv_styles"
                    android:text="姓名:"/>
                <ImageView
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/redsure" />
                <EditText
                    android:layout_toRightOf="@id/tv_verifyInfo_name"
                    android:id="@+id/et_verifyInfo_username"
                    style="@style/claimInfo_et_styles"
                    />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:gravity="center_vertical">
            <TextView
                android:id="@+id/tv_verifyInfo_phone"
                style="@style/claimInfo_tv_styles"
                android:text="电话:"/>
                <ImageView
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/redsure" />
            <EditText
                android:layout_toRightOf="@id/tv_verifyInfo_phone"
                android:id="@+id/et_verifyInfo_mobile"
                style="@style/claimInfo_et_styles"
                />
        </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="性别:" />
                <ImageView
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/redsure" />
                <RadioGroup
                    android:id="@+id/rg_verifyInfo_sex"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <RadioButton
                        android:id="@+id/rb_verifyInfo_man"
                        style="@style/claimInfo_tv_styles"
                        android:textSize="40px"
                        android:text="男" />
                    <RadioButton
                        android:id="@+id/rb_verifyInfo_women"
                        style="@style/claimInfo_tv_styles"
                        android:textSize="40px"
                        android:text="女" />
                </RadioGroup>
            </com.zhy.autolayout.AutoLinearLayout>
            <RadioGroup
                android:id="@+id/rg_verifyInfo_like"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_height="wrap_content"
                    style="@style/claimInfo_item_ll_styles"
                    >
                    <TextView
                        style="@style/claimInfo_tv_styles"
                        android:text="爱好:"
                        />
                    <ImageView
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/redsure" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_1"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="舞蹈" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_2"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="音乐"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_3"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="画画"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_4"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="乐器"/>
                </com.zhy.autolayout.AutoLinearLayout>
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_marginTop="14px"
                    android:layout_height="wrap_content"
                    style="@style/claimInfo_item_ll_styles"
                    >
                    <TextView
                        style="@style/claimInfo_tv_styles"
                        android:text="爱好:"
                        android:visibility="invisible"
                        />
                    <ImageView
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="invisible"
                        android:src="@mipmap/redsure" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_5"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="游戏" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_6"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="影视"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_7"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="旅行"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_12"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="棋类"/>
                </com.zhy.autolayout.AutoLinearLayout>
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_marginTop="14px"
                    android:layout_height="wrap_content"
                    style="@style/claimInfo_item_ll_styles"
                    >
                    <TextView
                        style="@style/claimInfo_tv_styles"
                        android:text="爱好:"
                        android:textColor="@color/white"
                        android:visibility="invisible"
                        />
                    <ImageView
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="invisible"
                        android:src="@mipmap/redsure" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_8"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="美食" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_9"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="社交"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_10"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="阅读"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_11"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="运动"/>
                </com.zhy.autolayout.AutoLinearLayout>
            </RadioGroup>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="地址:" />
                <ImageView
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/redsure" />
                <Spinner
                    style="@style/claimInfo_item_sp_styles"
                    android:id="@+id/sp_verifyInfo_province"
                    />
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="省"/>
                <Spinner
                    style="@style/claimInfo_item_sp_styles"
                    android:id="@+id/sp_verifyInfo_citys"
                    />
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="市"/>
                <Spinner
                    style="@style/claimInfo_item_sp_styles"
                    android:id="@+id/sp_verifyInfo_countys"
                    />
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="(区)/县"/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:id="@+id/ll_verifyInfo_birthday"
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="生日:"/>
                <ImageView
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/redsure" />
                <EditText
                    android:id="@+id/et_verifyInfo_birthday"
                    style="@style/claimInfo_et_styles"
                    android:focusableInTouchMode="false"
                    android:focusable="false"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles"
                android:layout_height="wrap_content">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="籍贯:" />
                <Spinner
                    style="@style/claimInfo_item_sp_styles"
                    android:id="@+id/sp_verifyInfo_jgprovince"
                    />
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="省"/>
                <Spinner
                    style="@style/claimInfo_item_sp_styles"
                    android:id="@+id/sp_verifyInfo_jgcitys"
                    />
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="市"/>
                <Spinner
                    style="@style/claimInfo_item_sp_styles"
                    android:id="@+id/sp_verifyInfo_jgcountys"
                    />
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="(区)/县"/>
            </com.zhy.autolayout.AutoLinearLayout>
                    <RadioGroup
                        android:id="@+id/rg_verifyInfo_character"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <com.zhy.autolayout.AutoLinearLayout
                            android:layout_height="wrap_content"
                            style="@style/claimInfo_item_ll_styles"
                            >
                            <TextView
                                style="@style/claimInfo_tv_styles"
                                android:text="性格:"
                                />
                            <CheckBox
                                android:id="@+id/rb_recom_character1"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="死宅" />
                            <CheckBox
                                android:id="@+id/rb_recom_character2"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="幽默"/>
                            <CheckBox
                                android:id="@+id/rb_recom_character3"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="漂亮"/>
                            <CheckBox
                                android:id="@+id/rb_recom_character4"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="大方"/>
                        </com.zhy.autolayout.AutoLinearLayout>
                        <com.zhy.autolayout.AutoLinearLayout
                            android:layout_marginTop="14px"
                            android:layout_height="wrap_content"
                            style="@style/claimInfo_item_ll_styles"
                            >
                            <TextView
                                style="@style/claimInfo_tv_styles"
                                android:text="性格:"
                                android:visibility="invisible"
                                />
                            <CheckBox
                                android:id="@+id/rb_recom_character5"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="急躁" />
                            <CheckBox
                                android:id="@+id/rb_recom_character6"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="轻浮"/>
                            <CheckBox
                                android:id="@+id/rb_recom_character7"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="花心"/>
                            <CheckBox
                                android:id="@+id/rb_recom_character12"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="逗比"/>
                        </com.zhy.autolayout.AutoLinearLayout>
                        <com.zhy.autolayout.AutoLinearLayout
                            android:layout_marginTop="14px"
                            android:layout_height="wrap_content"
                            style="@style/claimInfo_item_ll_styles"
                            >
                            <TextView
                                style="@style/claimInfo_tv_styles"
                                android:text="性格:"
                                android:textColor="@color/white"
                                android:visibility="invisible"
                                />
                            <CheckBox
                                android:id="@+id/rb_recom_character8"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="活泼" />
                            <CheckBox
                                android:id="@+id/rb_recom_character9"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="高冷"/>
                            <CheckBox
                                android:id="@+id/rb_recom_character10"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="诚信"/>
                            <CheckBox
                                android:id="@+id/rb_recom_character11"
                                style="@style/claimInfo_item_rb_styles"
                                android:text="可爱"/>
                        </com.zhy.autolayout.AutoLinearLayout>
                    </RadioGroup>

            <com.zhy.autolayout.AutoLinearLayout
            style="@style/claimInfo_item_ll_styles">
            <TextView
                style="@style/claimInfo_tv_styles"
                android:text="毕业学校:"/>
            <EditText
                android:layout_marginLeft="31px"
                android:id="@+id/et_verifyInfo_finishSchool"
                style="@style/claimInfo_et_styles"
                />
        </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="学历:"/>
                <EditText
                    android:layout_marginLeft="31px"
                    android:id="@+id/et_verifyInfo_degree"
                    style="@style/claimInfo_et_styles"
                    android:focusableInTouchMode="false"
                    android:focusable="false"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
                    <com.zhy.autolayout.AutoLinearLayout
                        style="@style/claimInfo_item_ll_styles">
                        <TextView
                            style="@style/claimInfo_tv_styles"
                            android:text="行业:"/>
                        <EditText
                            android:layout_marginLeft="31px"
                            android:id="@+id/et_verifyInfo_industry"
                            style="@style/claimInfo_et_styles"
                            android:focusableInTouchMode="false"
                            android:focusable="false"
                            />
                    </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="公司:"/>
                <EditText
                    android:layout_marginLeft="31px"
                    android:id="@+id/et_verifyInfo_company"
                    style="@style/claimInfo_et_styles"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="职位:"/>
                <EditText
                    android:layout_marginLeft="31px"
                    android:id="@+id/et_verifyInfo_position"
                    style="@style/claimInfo_et_styles"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="邮箱:"/>
                <EditText
                    android:layout_marginLeft="31px"
                    android:id="@+id/et_verifyInfo_email"
                    style="@style/claimInfo_et_styles"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="QQ:"/>
                <EditText
                    android:layout_marginLeft="31px"
                    android:id="@+id/et_verifyInfo_QQ"
                    style="@style/claimInfo_et_styles"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                style="@style/claimInfo_item_ll_styles">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="微信:"/>
                <EditText
                    android:layout_marginLeft="31px"
                    android:id="@+id/et_verifyInfo_wechat"
                    style="@style/claimInfo_et_styles"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
                    <com.zhy.autolayout.AutoLinearLayout
                        style="@style/claimInfo_item_ll_styles">
                        <TextView
                            style="@style/claimInfo_tv_styles"
                            android:text="父亲名字:"/>
                        <EditText
                            android:layout_marginLeft="31px"
                            android:id="@+id/et_verifyInfo_fatherName"
                            style="@style/claimInfo_et_styles"
                            />
                    </com.zhy.autolayout.AutoLinearLayout>
                    <com.zhy.autolayout.AutoLinearLayout
                        style="@style/claimInfo_item_ll_styles">
                        <TextView
                            style="@style/claimInfo_tv_styles"
                            android:text="母亲名字:"/>
                        <EditText
                            android:layout_marginLeft="31px"
                            android:id="@+id/et_verifyInfo_motherName"
                            style="@style/claimInfo_et_styles"
                            />
                    </com.zhy.autolayout.AutoLinearLayout>
                    <com.zhy.autolayout.AutoLinearLayout
                        style="@style/claimInfo_item_ll_styles">
                        <TextView
                            style="@style/claimInfo_tv_styles"
                            android:text="婚姻状况:" />
                        <RadioGroup
                            android:id="@+id/rg_verifyInfo_marriage"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_weight="1">
                            <RadioButton
                                android:id="@+id/rb_verifyInfo_unmarriage"
                                style="@style/claimInfo_tv_styles"
                                android:textSize="40px"
                                android:text="未婚" />
                            <RadioButton
                                android:id="@+id/rb_verifyInfo_marriage"
                                style="@style/claimInfo_tv_styles"
                                android:textSize="40px"
                                android:text="已婚" />
                        </RadioGroup>
                    </com.zhy.autolayout.AutoLinearLayout>
                    <com.zhy.autolayout.AutoLinearLayout
                        android:id="@+id/ll_verifyInfo_marriaged"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:orientation="vertical">
                        <com.zhy.autolayout.AutoLinearLayout
                            style="@style/claimInfo_item_ll_styles">
                            <TextView
                                style="@style/claimInfo_tv_styles"
                                android:text="配偶名字:" />
                            <EditText
                                android:id="@+id/et_verifyInfo_spouseName"
                                style="@style/claimInfo_et_styles" />
                        </com.zhy.autolayout.AutoLinearLayout>
                        <com.zhy.autolayout.AutoLinearLayout
                            style="@style/claimInfo_item_ll_styles">
                            <TextView
                                style="@style/claimInfo_tv_styles"
                                android:text="子女名字:" />
                            <EditText
                                android:id="@+id/et_verifyInfo_childrenName"
                                style="@style/claimInfo_et_styles" />
                        </com.zhy.autolayout.AutoLinearLayout>
                        <com.zhy.autolayout.AutoLinearLayout
                            style="@style/claimInfo_item_ll_styles">
                            <TextView
                                style="@style/claimInfo_tv_styles"
                                android:text="子女就读院校:" />
                            <EditText
                                android:id="@+id/et_verifyInfo_childrenSchool"
                                style="@style/claimInfo_et_styles" />
                        </com.zhy.autolayout.AutoLinearLayout>
                    </com.zhy.autolayout.AutoLinearLayout>
            <Button
                android:layout_marginTop="30px"
                android:id="@+id/btn_verifyRecommedInfo"
                android:gravity="center"
                android:layout_width="710px"
                android:layout_height="85px"
                android:textColor="@color/col_444343"
                android:background="@drawable/btn_login"
                android:text="确认信息"/>
        </com.zhy.autolayout.AutoLinearLayout>
    </ScrollView>

</com.zhy.autolayout.AutoLinearLayout>
