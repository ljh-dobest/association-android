<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/white">
        <TextView
            android:layout_marginLeft="20px"
            android:id="@+id/tv_user_detail_more_info_back"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:text="取消"/>
        <TextView
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="详细资料"
            android:textColor="@color/black"
            android:textSize="16sp" />

    </com.zhy.autolayout.AutoRelativeLayout>
    <View
        android:layout_width="match_parent"
        android:background="@color/col_999999"
        android:layout_height="1dp"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">
        <com.zhy.autolayout.AutoLinearLayout
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="姓名:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_name"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="QQ:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_QQ"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="微信:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_wechat"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <RadioGroup
                android:id="@+id/rg_user_detail_more_like"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_height="wrap_content"
                    style="@style/claimInfo_item_ll_styles"
                    >
                    <TextView
                        style="@style/claimInfo_tv_styles"
                        android:text="爱好:"
                        />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_1"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="舞蹈" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_2"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="音乐"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_3"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="画画"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_4"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="乐器"/>
                </com.zhy.autolayout.AutoLinearLayout>
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_marginTop="14px"
                    android:layout_height="wrap_content"
                    style="@style/claimInfo_item_ll_styles"
                    >
                    <TextView
                        style="@style/claimInfo_tv_styles"
                        android:text="爱好:"
                        android:visibility="invisible"
                        />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_5"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="游戏" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_6"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="影视"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_7"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="旅行"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_12"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="棋类"/>
                </com.zhy.autolayout.AutoLinearLayout>
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_marginTop="14px"
                    android:layout_height="wrap_content"
                    style="@style/claimInfo_item_ll_styles"
                    >
                    <TextView
                        style="@style/claimInfo_tv_styles"
                        android:text="爱好:"
                        android:textColor="@color/white"
                        android:visibility="invisible"
                        />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_8"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="美食" />
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_9"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="社交"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_10"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="阅读"/>
                    <CheckBox
                        android:id="@+id/rb_verifyInfo_11"
                        style="@style/claimInfo_item_rb_styles"
                        android:text="运动"/>
                </com.zhy.autolayout.AutoLinearLayout>
            </RadioGroup>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="毕业学校:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_school"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>

            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="星座:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_constellation"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="血型:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_bloodtype"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="公司:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_company"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="公司职位:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_position"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>

            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="婚姻状况:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_marriage"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>

            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>
    </ScrollView>

</com.zhy.autolayout.AutoLinearLayout>
