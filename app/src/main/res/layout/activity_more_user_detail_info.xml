                    style="@style/claimInfo_tv_styles"
                    android:text="生日:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_birthday"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="QQ:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_QQ"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="微信:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_wechat"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="籍贯:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_homeplace"
                    style="@style/claimInfo_et_styles"
                    android:text=""/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="100px"
                android:gravity="center_vertical">
                <TextView
                    style="@style/claimInfo_tv_styles"
                    android:text="毕业学校:"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/tv_user_detail_more_info_school"
