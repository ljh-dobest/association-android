<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ike.sq.commonwealactives"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rest"
        android:label="@string/name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.ike.sq.commonwealactives.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ike.sq.commonwealactives.ui.activity.FeedForCommentActivity" />
        <activity android:name="com.ike.sq.commonwealactives.ui.activity.MessageActivity" />
        <activity android:name="com.ike.sq.commonwealactives.ui.activity.BenefitParticularsActivity" />
        <activity android:name="com.ike.sq.commonwealactives.ui.activity.MineBenefitActivity" />
        <activity android:name="com.ike.sq.commonwealactives.ui.activity.BenefitRegisteredActivity" />

        <meta-data
            android:name="design_width"
            android:value="750" />
        <meta-data
            android:name="design_height"
            android:value="1340" />
    </application>

</manifest>